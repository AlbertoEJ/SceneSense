<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Eye shape filled with teal gradient -->
    <path
        android:pathData="M12,54 Q54,24 96,54 Q54,84 12,54Z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="linear"
                android:startX="12"
                android:startY="54"
                android:endX="96"
                android:endY="54">
                <item android:offset="0.0" android:color="#FF008899" />
                <item android:offset="0.5" android:color="#FF005566" />
                <item android:offset="1.0" android:color="#FF008899" />
            </gradient>
        </aapt:attr>
    </path>

    <!-- Eye outline -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#00AABE"
        android:strokeWidth="2"
        android:pathData="M12,54 Q54,24 96,54 Q54,84 12,54Z" />

    <!-- Iris outer ring -->
    <path
        android:pathData="M54,54m-17,0a17,17 0,1 1,34 0a17,17 0,1 1,-34 0">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="radial"
                android:centerX="54"
                android:centerY="54"
                android:gradientRadius="17">
                <item android:offset="0.0" android:color="#FF000000" />
                <item android:offset="0.45" android:color="#FF000000" />
                <item android:offset="0.6" android:color="#FF003246" />
                <item android:offset="0.78" android:color="#FF00A0BE" />
                <item android:offset="0.88" android:color="#FF00D2E6" />
                <item android:offset="1.0" android:color="#FF00788C" />
            </gradient>
        </aapt:attr>
    </path>

    <!-- Iris ring line -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#00C8DC"
        android:strokeWidth="1.2"
        android:pathData="M54,54m-17,0a17,17 0,1 1,34 0a17,17 0,1 1,-34 0" />

    <!-- Inner detail ring -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#008CA0"
        android:strokeWidth="0.8"
        android:pathData="M54,54m-12,0a12,12 0,1 1,24 0a12,12 0,1 1,-24 0" />

    <!-- Pupil -->
    <path
        android:pathData="M54,54m-8,0a8,8 0,1 1,16 0a8,8 0,1 1,-16 0">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="radial"
                android:centerX="54"
                android:centerY="54"
                android:gradientRadius="8">
                <item android:offset="0.0" android:color="#FFF0FAFF" />
                <item android:offset="0.3" android:color="#FF00BED2" />
                <item android:offset="0.6" android:color="#FF005A78" />
                <item android:offset="1.0" android:color="#FF000810" />
            </gradient>
        </aapt:attr>
    </path>

    <!-- Center bright dot -->
    <path
        android:fillColor="#F0FAFF"
        android:pathData="M54,54m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />

    <!-- Main reflection -->
    <path
        android:fillColor="#E6F5FF"
        android:pathData="M49,49m-2.8,0a2.8,2.8 0,1 1,5.6 0a2.8,2.8 0,1 1,-5.6 0" />

    <!-- Small reflection -->
    <path
        android:fillColor="#B4D2E6"
        android:pathData="M59,58m-1.3,0a1.3,1.3 0,1 1,2.6 0a1.3,1.3 0,1 1,-2.6 0" />

</vector>
